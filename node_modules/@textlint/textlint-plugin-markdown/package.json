        "test": "mocha \"test/**/*.{js,ts}\"",
        "watch": "tsc -b --watch"
    },
    "dependencies": {
        "@textlint/markdown-to-ast": "^12.6.1"
    },
    "devDependencies": {
        "@textlint/kernel": "^12.6.1",
        "cross-env": "^7.0.3",
        "mocha": "^9.2.2",
        "power-assert": "^1.6.1",
        "rimraf": "^3.0.2",
        "textlint-rule-no-todo": "^2.0.1",
        "ts-node-test-register": "^10.0.0",
        "typescript": "~4.9.4"
    },
    "publishConfig": {
        "access": "public"
    },
